        <attr name="etv_font" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="medium" value="2" />
            <enum name="italic" value="3" />
            <enum name="thin" value="4" />
            <enum name="bold" value="5" />
            <enum name="semi_bold" value="6" />
            <enum name="black" value="7" />
            <enum name="black_italic" value="8" />
            <enum name="bold_italic" value="9" />
            <enum name="regular_italic" value="10" />
            <enum name="ultra_light" value="11" />
            <enum name="light_italic" value="12" />
            <enum name="other_type_1" value="13" />
            <enum name="other_type_2" value="14" />
            <enum name="other_type_3" value="15" />
            <enum name="other_type_4" value="16" />
